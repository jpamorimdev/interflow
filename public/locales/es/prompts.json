          "name": "programar_evento",
          "description": "Programa un nuevo evento en el calendario",
          "parameters": {
            "title": {
              "description": "Título del evento",
              "type": "string",
              "example": "Reunión con el equipo de marketing"
            },
            "start_date": {
              "description": "Fecha y hora de inicio (formato ISO)",
              "type": "string",
              "example": "2023-11-15T14:00:00"
            },
            "end_date": {
              "description": "Fecha y hora de finalización (formato ISO)",
              "type": "string",
              "example": "2023-11-15T15:30:00"
            },
            "participants": {
              "description": "Lista de participantes (correos electrónicos)",
              "type": "array",
              "items": {
                "type": "string"
              },
              "example": ["maria@empresa.com", "juan@empresa.com"]
            }
          }
        },
        "translate": {
          "name": "traducir_texto",
          "description": "Traduce texto a otro idioma",
          "parameters": {
            "text": {
              "description": "El texto a traducir",
              "type": "string",
              "example": "Hola, ¿cómo estás hoy?"
            },
            "target_language": {
              "description": "El idioma al que traducir",
              "type": "string",
              "enum": ["Inglés", "Español", "Francés", "Alemán", "Italiano", "Japonés", "Chino"],
